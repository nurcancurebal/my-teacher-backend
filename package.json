{
  "name": "my-teacher-backend",
  "description": "My Teacher Backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run format && npm run lint && npm run build-ts",
    "start": "node dist",
    "dev": "ts-node src",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "8.2.0",
    "express": "^4.18.2",
    "joi": "^17.6.4",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "otplib": "^12.0.1",
    "pg": "^8.13.0",
    "sequelize": "^6.25.3",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.11",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.170",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.8.4",
    "@types/nodemailer": "6.4.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "7.19.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
